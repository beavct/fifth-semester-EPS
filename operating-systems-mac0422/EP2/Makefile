PP=gcc
COMPFLAGS= -Wall -pedantic -O2

###################################################################################

EP2_OBJ= ep2.o

###################################################################################


all: ep2

ep2.o: ep2.h

ep2: ${EP2_OBJ}
	${PP} ${COMPFLAGS} -o ep2 -O ep2.c -lpthread 
	@rm -rf *.o%    